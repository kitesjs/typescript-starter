{
  "name": "kites-typescript-starter",
  "version": "1.0.0",
  "description": "Kites TypeScript starter repository",
  "main": "app",
  "scripts": {
    "test": "mocha",
    "start": "ts-node-dev --respawn --transpile-only app.ts",
    "start:prod": "node dist/app.js",
    "build": "tsc -p tsconfig.build.json",
    "lint": "tslint -p tsconfig.json -c tslint.json app.ts \"todo/**/*.ts\" -e \"*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitesjs/typescript-starter.git"
  },
  "keywords": [
    "kitesjs",
    "starter"
  ],
  "author": "vunb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kitesjs/typescript-starter/issues"
  },
  "homepage": "https://github.com/kitesjs/typescript-starter#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.0.27",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@kites/common": "^1.3.1",
    "@kites/core": "^1.3.1",
    "@kites/express": "^1.3.1",
    "@kites/rest": "^1.3.1",
    "reflect-metadata": "^0.1.13"
  }
}
